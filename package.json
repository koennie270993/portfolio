{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Portfolio project",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:webpack": "webpack --config webpack.config.js",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "serve": "npm run build:webpack && bundle exec jekyll serve",
    "jekyll:serve": "bundle exec jekyll serve",
    "watch:ts": "webpack --watch --config webpack.config.js",
    "watch:jekyll": "bundle exec jekyll serve",
    "watch": "npm-run-all --parallel watch:*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:gh-pages": "npm run build:webpack && bundle exec jekyll build"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.32",
    "@types/node": "^20.11.18",
    "npm-run-all": "^4.1.5",
    "sharp": "^0.34.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.99.5",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "bootstrap-validator": "^0.11.9",
    "jquery": "^3.7.1",
    "jquery.easing": "^1.4.1"
  }
}
